{
  "manifest_version": 2,
  "name": "snake-game-firefox-ext",
  "description": "Play the snake game in devtools panel",
  "version": "1.0",
  "author": "vansteki",
  "homepage_url": "https://github.com/vansteki/snake-game-firefox-ext/",
  "icons": {
    "64": "icons/icon.png"
  },
  "background": {
    "scripts": ["background_scripts/background.js"]
  },
  "permissions": [
      "<all_urls>"
  ],
  "devtools_page": "/devtools/devtools-page.html"
}
